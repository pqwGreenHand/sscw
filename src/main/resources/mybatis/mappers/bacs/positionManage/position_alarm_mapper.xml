<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhixin.zhfz.bacs.dao.positionManage.IPositionAlarmMapper" >
  <resultMap id="BaseResultMap" type="com.zhixin.zhfz.bacs.entity.PositionAlarmEntity" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="cuff_id" property="cuffId" jdbcType="INTEGER" />
    <result column="cuff_no" property="cuffNo" jdbcType="VARCHAR" />
    <result column="device_id" property="deviceId" jdbcType="INTEGER" />
    <result column="device_no" property="deviceNo" jdbcType="INTEGER" />
    <result column="alarm_type" property="alarmType" jdbcType="INTEGER" />
    <result column="alarm_time" property="alarmTime" jdbcType="TIMESTAMP" />
  </resultMap>
  <select id="list" resultMap="BaseResultMap" parameterType="java.util.Map">
    select * from ba_position_alarm  
    where cuff_id=#{cuffId} and alarm_time &gt;=#{startTime} and
			alarm_time  &lt;=#{endTime}
    order by alarm_time desc
   
     limit ${pageStart},${rows}
  </select>

  <select id="count" resultType="java.lang.Integer" parameterType="java.util.Map">
       select count(1) from  ba_position_alarm  
       where cuff_id=#{cuffId} and alarm_time &gt;=#{startTime} and
			alarm_time  &lt;=#{endTime}
  </select>
</mapper>