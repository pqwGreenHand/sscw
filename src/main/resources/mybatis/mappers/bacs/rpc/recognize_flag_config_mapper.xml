<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.zhixin.zhfz.bacs.dao.rpc.IRecognizeFlagConfigMapper">
	<resultMap id="resultMap" type="com.zhixin.zhfz.bacs.entity.RecognizeFlagConfigEntity">
    	<id property="id" column="id"/>
    	<result property="type" column="type"/>
    	<result property="keyName" column="key_name"/>
    	<result property="keyValue" column="key_value"/>
    	<result property="sortNo" column="sort_no"/>
    </resultMap>

	<select id="all" parameterType="java.util.Map" resultMap="resultMap">
		SELECT f.*  from `ba_recognize_flag_config` AS f ORDER BY f.type,f.sort_no
	</select>


</mapper>


