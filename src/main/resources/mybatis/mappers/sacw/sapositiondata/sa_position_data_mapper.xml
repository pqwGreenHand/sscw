<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhixin.zhfz.sacw.dao.sapositiondata.ISaPositionDataMapper">
    <resultMap id="BaseResultMap" type="com.zhixin.zhfz.sacw.entity.SaPositionDataEntity">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="label_id" property="labelId" jdbcType="INTEGER"/>
        <result column="label_no" property="labelNo" jdbcType="VARCHAR"/>
        <result column="device_id" property="deviceId" jdbcType="INTEGER"/>
        <result column="device_no" property="deviceNo" jdbcType="INTEGER"/>
        <result column="send_time" property="sendTime" jdbcType="TIMESTAMP"/>
        <result column="low_power" property="lowPower" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="insert" parameterType="com.zhixin.zhfz.sacw.entity.SaPositionDataEntity" keyProperty="id"
            useGeneratedKeys="true">
        insert into sa_position_data (label_id, device_id, location_id,warehouse_id, send_time,low_power) values
        (#{labelId},#{deviceId},#{locationId},#{warehouseId},now(),#{lowPower})
    </insert>
</mapper>